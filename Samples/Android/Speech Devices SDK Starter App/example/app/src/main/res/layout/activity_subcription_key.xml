<?xml version="1.0" encoding="utf-8"?><android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    tools:context=".subcriptionKey">    <LinearLayout        android:layout_width="match_parent"        android:layout_height="match_parent"        android:orientation="vertical">        <LinearLayout            android:layout_width="match_parent"            android:layout_height="60dp"            android:orientation="horizontal">            <TextView                android:id="@+id/textView2"                android:layout_width="wrap_content"                android:layout_height="match_parent"                android:layout_weight="1"                android:gravity="center"                android:text="Subscription Keys"                android:textAlignment="center"                android:textAllCaps="true"                android:textColor="@android:color/black"                android:textSize="18sp"                android:textStyle="bold" />        </LinearLayout>        <LinearLayout            android:layout_width="match_parent"            android:layout_height="60dp"            android:orientation="horizontal">            <TextView                android:id="@+id/textView3"                android:layout_width="80dp"                android:layout_height="match_parent"                android:layout_weight="1"                android:gravity="center"                android:text="Speech Key: "                android:textAlignment="center"                android:textAllCaps="true"                android:textColor="@android:color/black"                android:textSize="14sp"                android:textStyle="bold" />            <EditText                android:id="@+id/speechKeyEditText"                android:layout_width="wrap_content"                android:layout_height="match_parent"                android:layout_weight="3"                android:ems="10"                android:hint="Please input speech key"                android:inputType="text"                android:textSize="12sp" />        </LinearLayout>        <LinearLayout            android:layout_width="match_parent"            android:layout_height="60dp"            android:orientation="horizontal">            <TextView                android:id="@+id/textView4"                android:layout_width="80dp"                android:layout_height="match_parent"                android:layout_weight="1"                android:gravity="center"                android:text="Speech Region: "                android:textAlignment="center"                android:textAllCaps="true"                android:textColor="@android:color/black"                android:textSize="14sp"                android:textStyle="bold" />            <EditText                android:id="@+id/speechRegionEditText"                android:layout_width="wrap_content"                android:layout_height="match_parent"                android:layout_weight="3"                android:ems="10"                android:hint="westus"                android:inputType="text"                android:textSize="12sp" />        </LinearLayout>        <LinearLayout            android:layout_width="match_parent"            android:layout_height="60dp"            android:orientation="horizontal">            <TextView                android:id="@+id/textView5"                android:layout_width="80dp"                android:layout_height="match_parent"                android:layout_weight="1"                android:gravity="center"                android:text="Luis Subscription Key: "                android:textAlignment="center"                android:textAllCaps="true"                android:textColor="@android:color/black"                android:textSize="14sp"                android:textStyle="bold" />            <EditText                android:id="@+id/luisKeyEditText"                android:layout_width="wrap_content"                android:layout_height="match_parent"                android:layout_weight="3"                android:ems="10"                android:hint="Please input Luis Subscription Key"                android:inputType="text"                android:textSize="12sp" />        </LinearLayout>        <LinearLayout            android:layout_width="match_parent"            android:layout_height="60dp"            android:orientation="horizontal">            <TextView                android:id="@+id/textView6"                android:layout_width="80dp"                android:layout_height="match_parent"                android:layout_weight="1"                android:gravity="center"                android:text="Luis Region: "                android:textAlignment="center"                android:textAllCaps="true"                android:textColor="@android:color/black"                android:textSize="14sp"                android:textStyle="bold" />            <EditText                android:id="@+id/luisRegionEditText"                android:layout_width="wrap_content"                android:layout_height="match_parent"                android:layout_weight="3"                android:ems="10"                android:hint="westus2"                android:inputType="text"                android:textSize="12sp" />        </LinearLayout>        <LinearLayout            android:layout_width="match_parent"            android:layout_height="60dp"            android:orientation="horizontal">            <TextView                android:id="@+id/textView7"                android:layout_width="80dp"                android:layout_height="match_parent"                android:layout_weight="1"                android:gravity="center"                android:text="Luis AppId: "                android:textAlignment="center"                android:textAllCaps="true"                android:textColor="@android:color/black"                android:textSize="14sp"                android:textStyle="bold" />            <EditText                android:id="@+id/luisAppIdEditText"                android:layout_width="wrap_content"                android:layout_height="match_parent"                android:layout_weight="3"                android:ems="10"                android:hint="Please input Luis AppId"                android:inputType="text"                android:textSize="12sp" />        </LinearLayout>    <LinearLayout        android:layout_width="match_parent"        android:layout_height="40dp"        android:orientation="horizontal">    </LinearLayout>    <LinearLayout            android:layout_width="match_parent"            android:layout_height="60dp"            android:orientation="horizontal">            <Button                android:id="@+id/resetButton"                android:layout_width="80dp"                android:layout_height="wrap_content"                android:layout_weight="1"                android:text="Reset" />            <Button                android:id="@+id/okButton"                android:layout_width="80dp"                android:layout_height="wrap_content"                android:layout_weight="1"                android:text="Ok" />            <Button                android:id="@+id/returnButton"                android:layout_width="80dp"                android:layout_height="wrap_content"                android:layout_weight="1"                android:text="Return" />        </LinearLayout>        <LinearLayout            android:layout_width="match_parent"            android:layout_height="80dp"            android:orientation="horizontal">            <TextView                android:id="@+id/warningTextView"                android:layout_width="wrap_content"                android:layout_height="match_parent"                android:layout_weight="1"                android:gravity="center"                android:text=""                android:textAlignment="center"                android:textAllCaps="true"                android:textColor="@android:color/black"                android:textSize="18sp"                android:textStyle="bold" />        </LinearLayout>    </LinearLayout></android.support.constraint.ConstraintLayout>